<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane minHeight="177.0" minWidth="450.0" prefHeight="177.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.baylor.ecs.csi.ExitMain">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <top>
        <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
            </padding>
         <children>
            <Text fill="#d8d8d8" strokeType="OUTSIDE" strokeWidth="0.0" text="Are you sure you want to leave?">
               <font>
                  <Font size="15.0" />
               </font></Text>
         </children>
        </HBox>
    </top>
   <left>
      <Button fx:id="yes" mnemonicParsing="false" onAction="#yesAction" prefHeight="32.0" prefWidth="182.0" text="Yes" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Button fx:id="no" mnemonicParsing="false" onAction="#noAction" prefHeight="32.0" prefWidth="182.0" text="No" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
